<Group hide-output="false" labelreference="L38" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">RationaleZerlegung:=proc(r::ratpoly,x::symbol)
   local p,q,P,a,b,c,d,s,Lsg,k,alpha,beta;
   p:=numer(r);
   q:=denom(r);
   P:=quo(p,q,x);
   p:=rem(p,q,x);
   b:=gcd(q,diff(q,x));
   d:=normal(q/b);
   a:=add(alpha[k]*x^k,k=0..degree(b,x)-1);
   c:=add(beta[k]*x^k,k=0..degree(d,x)-1);
   s:=normal(p-q*(diff(a/b,x)+c/d));
   Lsg:=solve({coeffs(s,x)},{alpha[k] $ k=0..degree(b,x)-1} union 
        {beta[k] $ k=0..degree(d,x)-1});
   a:=subs(Lsg,a);
   c:=subs(Lsg,c);
   [int(P,x)+factor(normal(a/b)),normal(c/d)]
end proc:
</Text-field>
</Input>
</Group>
