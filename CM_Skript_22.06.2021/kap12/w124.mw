<Text-field style="Diagnostic" layout="Diagnostic">{--&gt; enter codegen:-GRADIENT, args = g, [x, y]
{--&gt; enter codegen/GRAD, args = g, [x, y]
{--&gt; enter GRAD/reverse, args = g, [x, y], [], seq, false
&lt;-- ERROR in GRAD/reverse (now in codegen/GRAD) = Procedure not reversable}
{--&gt; enter GRAD/forward, args = g, [x, y], [], seq, false</Text-field>
<Text-field style="Diagnostic" layout="Diagnostic">&lt;-- exit GRAD/forward (now in codegen/GRAD) = proc (x, y, n) local dt, i, t; dt := array(1 .. 2); dt[1] := y; dt[2] := x; t := x*y; for i to n do dt[1] := t*y+x*y*dt[1]; dt[2] := t*x+x*y*dt[2]; t := t*x*y end do; if x &lt; y then return t+dt[1]*x, x*dt[2] else return y*dt[1], t+dt[2]*y end if end proc}
{--&gt; enter forget:-ModuleApply, args = tools/rename
&lt;-- exit forget:-ModuleApply (now in codegen/GRAD) = }
{--&gt; enter forget:-ModuleApply, args = tools/unrename
&lt;-- exit forget:-ModuleApply (now in codegen/GRAD) = }
&lt;-- exit codegen/GRAD (now in codegen:-GRADIENT) = proc (x, y, n) local dt, i, t; dt := array(1 .. 2); dt[1] := y; dt[2] := x; t := x*y; for i to n do dt[1] := t*y+x*y*dt[1]; dt[2] := t*x+x*y*dt[2]; t := t*x*y end do; if x &lt; y then return t+dt[1]*x, x*dt[2] else return y*dt[1], t+dt[2]*y end if end proc}
&lt;-- exit codegen:-GRADIENT (now at top level) = proc (x, y, n) local dt, i, t; dt := array(1 .. 2); dt[1] := y; dt[2] := x; t := x*y; for i to n do dt[1] := t*y+x*y*dt[1]; dt[2] := t*x+x*y*dt[2]; t := t*x*y end do; if x &lt; y then return t+dt[1]*x, x*dt[2] else return y*dt[1], t+dt[2]*y end if end proc}</Text-field>
</Output>
</Group>
