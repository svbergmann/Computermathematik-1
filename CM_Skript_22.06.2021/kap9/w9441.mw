<Group labelreference="L37" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Romberg:=proc(a::float,b::float,f::procedure,n::nonnegint)
   local h,t,m,q,k:
   h:=b-a:
   t[0]:=h*f((a+b)/2):
   for m from 1 to n do
      h:=h/3: 
      q:=1:
      t[m]:=t[m-1]/3+h*add(f(a+(3*k+1/2)*h)+f(a+(3*k+5/2)*h),k=0..3^(m-1)-1):
      for k from m-1 by -1 to 0 do
         q:=q*9:
         t[k]:=(t[k+1]*q-t[k])/(q-1):
      end do:
   end do:
   t[0]
end proc:
</Text-field>
</Input>
</Group>
