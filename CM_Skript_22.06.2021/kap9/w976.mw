<Group labelreference="L98" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ASM:=proc(N::posint,nmax::posint,a,b,m,alpha,eps)
     local u,k,du,fb,mb,g:
     g:=0:
     du:=0:
     fb:=b(2)/m(0):
     u:=alpha(0)/m(0):
     k:=1:
     while k&lt;=nmax do
        mb:=m(k)*fb:
        du:=fb*alpha(k)-g*du-mb*u: 
        g:=mb-a(k+1)+g:
        du:=du/g:
        u:=u+du:
        if k&gt;N and abs(du) &lt;= abs(u)*eps then
           return u,k
        end if:
        g:=b(k+2)/g:
        fb:=fb*g:
        k:=k+1:
     end do:
     u,k
end proc:
</Text-field>
</Input>
</Group>
