<Group hide-output="false" labelreference="L37" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Romberg:=proc(f::procedure,a::float,b::float,n::nonnegint)
   local h,t,m,q,k:
   h:=b-a:
   t[0]:=h/2*(f(a)+f(b)):
   for m from 1 to n do
      h:=h/2: 
      q:=1:
      t[m]:=t[m-1]/2+h*add(f(a+(2*k-1)*h),k=1..2^(m-1)):
      for k from m-1 by -1 to 0 do
         q:=q*4:
         t[k]:=(t[k]/q-t[k+1])/(1/q-1):
      end do:
   end do:
   t[0]
end proc:
</Text-field>
</Input>
</Group>
