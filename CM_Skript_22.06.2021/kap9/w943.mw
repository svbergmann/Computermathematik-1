<Group labelreference="L37" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GW:=proc(f,n::posint)
   local beta,T,e,v;
   option hfloat;
   uses LinearAlgebra:   
   beta:=[seq(k/sqrt(4.*k^2-1),k=1..n-1)];
   BandMatrix([beta,[seq(0,k=1..n)],beta]);
   T:=Matrix(%,shape=symmetric):
   (e,v):=Eigenvectors(T);
   add(2*Column(v,k)[1]^2*f(e[k]),k=1..n);
end proc:
