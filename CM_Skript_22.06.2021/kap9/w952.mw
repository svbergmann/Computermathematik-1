<Group labelreference="L26" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">RungeKutta:=proc(s::posint,m::posint)
   local TaylorPhi,RungeKuttaPhi,i,j,h,k,d,vars,eqns;
   global a,b,c;
   uses SolveTools;
   # Taylor-Reihe
   D(y):=x -&gt; f(x,y(x)):
   TaylorPhi:=convert(taylor(y(x+h),h=0,m+1),polynom):
   TaylorPhi:=normal((TaylorPhi-y(x))/h):
   # Runge-Kutta-Ansatz
   c[1]:=0:
   for i from 1 to s do
      k[i]:=f(x+c[i]*h,y(x)+h*add(a[i,j]*k[j],j=1..i-1))
   end do:
   RungeKuttaPhi:=convert(taylor(add(b[i]*k[i],i=1..s),h=0,m),polynom):        
   d:=expand(TaylorPhi-RungeKuttaPhi):
   vars:={seq(c[i],i=2..s),seq(b[i],i=1..s),
          seq((seq(a[i,j],j=1..i-1)),i=2..s)}:
   eqns:={coeffs(d,indets(d) minus vars)}:
   # Konsistenzbedingung
   eqns:=eqns union {seq(sum(a[i,j],j=1..i-1)-c[i],i=2..s)}:
   if m &lt; 4 then
      PolynomialSystem(eqns,vars,engine=triade):
   else
      PolynomialSystem(eqns,vars,engine=groebner):
   end if:
end proc:
</Text-field>
</Input>
</Group>
