<Group labelreference="L15" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">epsilon:=proc(n::nonnegint,xn::float,arr)
   local j,aux1,aux2,diff1,dmin,dmax;
   dmin:=evalhf(DBL_MIN);
   dmax:=evalhf(DBL_MAX);
   arr[n]:=xn;
   if n=0 then
      return xn
   else
      aux2:=0;
      for j from n by -1 to 1 do
         aux1,aux2:=aux2,arr[j-1];
         diff1:=arr[j] - aux2;
         if evalf(abs(diff1)) &lt; dmin then
            arr[j-1]:=dmax
         else
            arr[j-1]:=aux1 + 1/diff1
         end if
      end do
   end if:
   if modp(n,2)=0 then
      return arr[0]
   else
      return arr[1]
   end if
end proc:
</Text-field>
</Input>
</Group>
