<Group labelreference="L27" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Levin:=proc(n::nonnegint,xn::float,rn::float,arlo,arup)
   local j,fact,dmin,dmax;
   dmin:=evalhf(DBL_MIN);
   dmax:=evalhf(DBL_MAX);
   arlo[n]:=1/((n+1)*rn);
   arup[n]:=xn*arlo[n];
   for j from 1 to n do
      fact     :=(n-j+1)*n^(j-1)/(n+1)^j;
      arup[n-j]:=arup[n-j+1] - fact*arup[n-j];
      arlo[n-j]:=arlo[n-j+1] - fact*arlo[n-j]
   end do;
   if evalf(abs(arlo[0])) &lt; dmin then
      return dmax
   else
      return arup[0]/arlo[0]
   end if;
end proc:
</Text-field>
</Input>
</Group>
