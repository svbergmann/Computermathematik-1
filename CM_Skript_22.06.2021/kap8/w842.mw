<Group labelreference="L12" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">DiffImpl:=module()
   description &quot;Symbolische Differentiation&quot;;
   local functab,ruletab,diffProduct,diffPower;
   export differentiate,addFunc,addRule,rule;
   option package;
   # Funktionstabelle erweitern 
   addFunc:=proc(fname::symbol,fderiv::procedure)
      if assigned(functab[fname]) then
         error &quot;Funktion %1 ist bereits in der Funktionstabelle&quot;,
         fname
      else
         functab[fname]:=fderiv;
      end if;
   end proc;
<Font encoding="UTF-8">   # Ableitungsregel hinzuf\303\274gen
</Font>   addRule:=proc(T,impl)
      if type(T,{set,list}) then
         map(procname,_passed)
<Font encoding="UTF-8">      elif type(T,And(name,type)) then # g\303\274ltiger Typname
</Font>         ruletab[T]:=impl
      else
         error &quot;Datentyp name erwartet, %1&quot;,T
      end if
   end proc;
<Font encoding="UTF-8">   # Ableitungsregel f\303\274r einen bestimmten Ausdruckstyp </Font>abfragen
   rule:=proc(T)
	if type(T,And(name,type)) then
	   if assigned(ruletab[T]) then
	      eval(ruletab[T],1)
	   else
<Font encoding="UTF-8">	     error &quot;keine Ableitungsregel f\303\274r diesen </Font>Ausdrucktyp %1&quot;, T
          end if
       else
          error &quot;Datentyp name erwartet, %1&quot;, T
       end if
   end proc;
   #Ableitungsprozedur
   differentiate:=proc(expr,var::name)
      description &quot;Berechnung der Ableitung einer Funktion mit 
                   einer Variablen&quot;;
      local a,b,e;
      #Differentiationsregeln
      if not has(expr,var) then
         0
      elif expr=var then
         1
      elif type(expr,function) and nops(expr)=1 then
         e:=op(0,expr); # Name der Funktion
         a:=op(1,expr); # das Argument
         if assigned(functab[e]) then
            functab[e](a) * procname(a,var)
         else
            'procname(_passed)'
         end if
      else
         b:=whattype(expr);
         if assigned(ruletab[b]) then
            ruletab[b](expr,var)
         else
            'procname(_passed)'
         end if
      end if
   end proc;
   # Ableitungsregeln
   # komponentenweises Ableiten 
   addRule({list,set,tabular},()-&gt;map(differentiate,_passed));
   # Summenregel
   addRule(`+`, ()-&gt;map(differentiate,_passed));
   # verallgemeinerte Produktregel
   diffProduct:=proc(expr,var)
      local i;
      #add(subsop(i = differentiate(op(i,expr),var),expr),
      #	i = 1..nops(expr));
      add(differentiate(op(i,expr),var) * subsop(i=1,expr ),
	i = 1..nops(expr))
   end proc;
   addRule(`*`,eval(diffProduct));
   # Potenzregel
   diffPower:=proc(expr,var)
      local b,e;
      b,e:=op(expr); 
      if has(e,var) then
         expr * (differentiate(e,var) * ln(b)
         + e * differentiate(b,var)/b) 
      else #  einfachere Formel      
         e * b^(e-1) * differentiate(b,var) 
      end if
   end proc; 
   addRule(`^`,eval(diffPower));  
   # Ableitungen einiger Funktionen
   addFunc(sin,x-&gt;cos(x));
   addFunc(cos,x-&gt;-sin(x));
   addFunc(exp,x-&gt;exp(x));
   addFunc(ln,x-&gt;1/x):
end module;</Text-field>
</Input>
