<Group labelreference="L5" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">differentiate:=proc(expr,var)
   local a,b;
   #Differentiationsregeln
   if type(expr,constant) then
      0
   elif expr=var then
      1
   elif type(expr,`+`) then
      map(procname,_passed)
   elif type(expr,`^`) then
      a,b:=op(expr);
      if a=var and not has(b,var) then
         b*a^(b-1)
      else
         'procname(_passed)'
      end if
   elif type(expr,`*`) then
      a,b:=op(1,expr),subsop(1=1,expr);
      procname(a,var)*b + a*procname(b,var)
   else
      'procname(_passed)'
   end if
end proc:
</Text-field>
</Input>
</Group>
