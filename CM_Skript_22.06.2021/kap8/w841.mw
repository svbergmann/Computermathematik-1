<Group labelreference="L18" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">differentiate:=proc(expr,var)
   local a,b;
   #Differentiationsregeln
   if not has(expr,var) then <Font encoding="UTF-8"># Ausdruck enth\303\244lt nicht die Differentiationsvariable</Font>
      0
   elif expr=var then
      1
   elif type(expr,`+`) then
      map(procname,_passed)
   elif type( expr,`^`) then
      a,b:=op(expr);
      if not has(b,var) then
         b*a^(b-1) * procname(a,var) # Anw. der Kettenregel auf Basis
      else
         'procname(_passed)'
      end if
   elif type(expr,`*`) then
      a,b:=op(1,expr), subsop(1=1,expr);
      procname(a,var) * b + a * procname(b,var)
   elif type(expr,'function') and nops(expr)=1 then
      #Funktionen mit einer Variablen; Kettenregel
      b:=op(0,expr); # Name der Funktion
      a:=op(1,expr); # das Argument
      if b = sin then
         cos(a) * procname(a,var)
      elif b = cos then
         -sin(a) * procname(a,var)
      elif b = exp then
         exp(a) * procname(a,var)
      elif b = ln then
         (1/a) * procname(a,var)
      end if 
   else
      'procname(_passed)'
   end if
end proc:
</Text-field>
</Input>
</Group>
