<Group labelreference="L27" drawlabel="true">
<Input><Text-field style="Normal" layout="Normal"><Font encoding="UTF-8">Das Hinzuf\303\274gen weiterer Funktionen ist aber umst\303\244ndlich, auch muss die Kettenregel jedesmal wieder implementiert werden. Ein besserer Weg ist, die Funktionen zusammen mit ihren Differentiationsregeln in einer Funktionstabelle (</Font><Hyperlink linktarget="Help:table" hyperlink="true"><Font style="Hyperlink" family="Arial">table</Font></Hyperlink>) zu halten:</Text-field><Text-field style="Normal" layout="Normal"></Text-field>
</Input>
</Group>
<Group labelreference="L28" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">differentiate:=proc(expr,var)
   local a,b,functab;
   # Funktionstabelle
   functab:=table();
   functab[sin]:=x-&gt;cos(x);
   functab[cos]:=x-&gt;-sin(x);
   functab[exp]:=x-&gt;exp(x);
   functab[ln] :=x-&gt;1/x:
   #Differentiationsregeln
   if not has (expr,var) then
      0
   elif expr=var then
      1
   elif type(expr,`+`) then
      map(procname,_passed)
   elif type( expr,`^`) then
      a,b:=op(expr);
      if not has(b,var) then
         b*a^(b-1) * procname(a,var)
      else
         'procname(_passed)'
      end if
   elif type(expr,`*`) then
      a,b:=op(1,expr), subsop(1=1,expr);
      procname(a,var)*b + a*procname(b,var)
   elif type(expr,function) and nops(expr)=1 then
      #Funktionen mit einer Variablen; Kettenregel
      b:=op(0, expr); # Name der Funktion
      a:=op(1, expr); # das Argument
      if assigned(functab[b]) then
         # Diese Funktion ist &quot;bekannt&quot;
         functab[b](a) * procname(a,var)
      else
         # Diese Funktion ist nicht bekannt; der Aufruf wird
<Font encoding="UTF-8">         # unausgewertet zur\303\274ckgegeben 
</Font>         'procname(_passed)'
      end if
   else
     'procname(_passed)'
   end if
end proc:
</Text-field>
</Input>
</Group>
