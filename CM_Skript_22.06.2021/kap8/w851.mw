<Group hide-output="false" labelreference="L15479" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">mySeq:=proc(f,i,n)
  local thisNode,N,result,j,div;
  uses Grid;
  #
  thisNode:=MyNode();
  N:=NumNodes();
  div:=floor(n/N);
  #
  result:=seq(eval(f,i=j),j=thisNode*div+1..(thisNode+1)*div):
  #
  if(thisNode&gt;0) then
    Send(0,result); 
  else
    result,seq(Receive(j),j=1..N-1),seq(eval(f,i=j),j=div*N+1..n);
  end if:
#
end proc:
</Text-field>
</Input>
