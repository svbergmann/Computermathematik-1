<Group hide-input="false" labelreference="L50" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field style="Text" layout="Normal"><Font bold="true" encoding="UTF-8" family="Arial">Die folgende Prozedur f\303\274hrt die Addition der nat\303\274rlichen Zahlen </Font><Font family="Monospaced">1 + 2 + ... + n</Font>  <Font bold="true" family="Arial">auf </Font><Font bold="true" family="Monospaced">N</Font><Font bold="true" family="Arial"> Knoten durch. Die Kommunikation (message passing) erfolgt mit  </Font><Hyperlink linktarget="Help:Send" hyperlink="true"><Font style="Hyperlink" family="Arial">Send()</Font></Hyperlink><Font bold="true" family="Arial"> und </Font><Hyperlink linktarget="Help:Receive" hyperlink="true"><Font style="Hyperlink" family="Arial">Receive()</Font></Hyperlink><Font bold="true" family="Arial">. Der Master-Knoten </Font><Font bold="true" family="Monospaced">0</Font><Font bold="true" family="Arial"> sammelt die Teilergebnisse.</Font></Text-field><Text-field style="Text" layout="Normal"></Text-field>
</Input>
</Group>
<Group hide-output="false" labelreference="L38" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">myAdd:=proc(n)
  local thisNode,N,result,i,div;
  uses Grid;
  #
  thisNode:=MyNode();
  N:=NumNodes();
  div:=floor(n/N);
  #
  result:=add(i,i=thisNode*div+1..(thisNode+1)*div):
  #
  if(thisNode&gt;0) then
    Send(0,result); 
  else
    result+add(Receive(i),i=1..N-1)+add(i,i=div*N+1..n);
  end if:
#
end proc:
</Text-field>
</Input>
</Group>
