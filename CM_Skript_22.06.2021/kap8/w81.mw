<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal268">if x &lt; 0 then 
  0 
else 
   1 
end if;     </Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output">IiIh</Equation></Text-field>
</Output>
</Group>
